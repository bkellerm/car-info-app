{
    "api": {
        "carinfo": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "authConfig": {
                    "additionalAuthenticationProviders": [],
                    "defaultAuthentication": {
                        "authenticationType": "API_KEY",
                        "apiKeyConfig": {
                            "description": "API key",
                            "apiKeyExpirationDays": "14"
                        }
                    }
                }
            }
        }
    },
    "function": {
        "resetCarTable": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "api",
                    "resourceName": "carinfo",
                    "attributes": [
                        "GraphQLAPIIdOutput"
                    ]
                }
            ]
        }
    }
}